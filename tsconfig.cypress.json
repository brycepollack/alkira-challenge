{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["es2020", "dom"],
    "module": "commonjs",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": false,
    "noEmit": true,
    "types": ["cypress"]
  },
  "include": ["cypress/**/*.ts", "cypress/**/*.tsx"]
}
